			"patterns": [
				{
					"name": "entity.name.function",
					"match": "guess I'll die"
				},
				{
					"name": "entity.name.function",
					"match": "confused stonks"
				},
				{
					"name": "entity.name.function",
					"match": "perfectly balanced as all things should be"
				},
				{
					"name": "entity.name.function",
					"match": "wait, that's illegal"
				}
			]
		},
		"debug-commands": {
			"patterns": [
				{
					"name": "entity.name.function",
					"match": "it's a trap"
				}
			]
		},
		"additions": {
			"patterns": [
				{
					"comment": "Make sure the beginning is a valid command that we know",
					"match": "(.+) (or draw 25)",
					"captures": {
						"1": {
							"patterns": [
								{
									"include": "#commands"
								}
							]
						},
						"2": {
							"name": "keyword.control"
						}
					}
				}
			]
		}
