		{
			"include": "#additions"
		},
		{
			"include": "#commands"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"commands": {
			"patterns": [
				{
					"include": "#stack-operations"
				},
				{
					"include": "#register-manipulation"
				},
				{
					"include": "#functions"
				},
				{
					"include": "#logical-operations"
				},
				{
					"include": "#arithmetic-operations"
				},
				{
					"include": "#io-operations"
				},
